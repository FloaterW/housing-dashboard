{
  "name": "housing-dashboard",
  "version": "1.0.0",
  "description": "A modern housing market dashboard for real-time analytics and insights",
  "author": "Your Name",
  "private": true,
  "dependencies": {
    "@arcgis/core": "^4.32.10",
    "@testing-library/dom": "^10.4.0",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "date-fns": "^4.1.0",
    "puppeteer": "^24.10.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.15.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,css,md}",
    "prepare": "husky",
    "setup": "node setup.js",
    "dev": "npm start",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "full:dev": "concurrently \"npm run backend:dev\" \"npm run dev\"",
    "clean": "rm -rf build node_modules backend/node_modules",
    "fresh-install": "npm run clean && npm install && cd backend && npm install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(axios)/)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "eslint-config-prettier": "^10.1.5",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jest-environment-jsdom": "^30.0.0",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.5",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17"
  }
}
